{
  "name": "clean-architecture-example",
  "version": "1.0.0",
  "scripts": {
    "test": "jest --passWithNoTests",
    "test-watch": "jest --passWithNoTests --watch",
    "test-coverage": "jest --passWithNoTests --coverage"
  },
  "keywords": [],
  "author": "Douglas Volcato",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "email-validator": "^2.0.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/email-validator": "^1.0.6",
    "@types/jest": "28.1.8",
    "@types/node": "^18.11.0",
    "@types/uuid": "^9.0.4",
    "jest": "28.1.3",
    "ts-jest": "28.0.8",
    "ts-node": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*index.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@/(.+)": "<rootDir>/$1"
    }
  },
  "compilerOptions": {
    "moduleResolution": "node"
  }
}
